<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean id="userService" class="com.gyf.service.Impl.UserServiceImpl">
        <!-- 依赖注入数据，调用属性的set方法 -->
        <property name="name" value="zhangsan"></property>
    </bean>

    <bean id="userService1" class="com.gyf.service.UserServiceFactory" factory-method="createUserService"></bean>

    <bean id="factory1" class="com.gyf.service.UserServiceFactory1"></bean>
    <bean id="userService2" factory-bean="factory1" factory-method="createUserService"></bean>

</beans>
