<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->

    <bean id="people2" class="com.gyf.model.People" scope="singleton">
        <!-- 依赖注入数据，调用属性的set方法 -->
        <constructor-arg name="age" value="33"  ></constructor-arg>
        <constructor-arg name="name" value="maleasd"  ></constructor-arg>
    </bean>

    <bean id="people1" class="com.gyf.model.People" scope="singleton">
        <!-- 依赖注入数据，调用属性的set方法 -->
        <constructor-arg name="age" value="33"  ></constructor-arg>
        <constructor-arg name="gender" value="male"  ></constructor-arg>

    </bean>



</beans>
